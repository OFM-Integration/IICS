      <types1:AutosaveExists>false</types1:AutosaveExists>
      <types1:Entry>
         <process xmlns="http://schemas.active-endpoints.com/appmodules/screenflow/2010/10/avosScreenflow.xsd"
                  xmlns:list="urn:activevos:spi:list:functions"
                  displayName="pr_MI"
                  name="pr_MI"
                  overrideAPIName="true">
            <appliesTo/>
            <description>Get result set from DB to attachment</description>
            <tags>.agent:SecureAgent,.anonymous</tags>
            <generator>Informatica Process Designer 11</generator>
            <output>
               <field description="" name="Response" type="attachment">
                  <options>
                     <option hide="true" name="count">1</option>
                     <option name="maxsize">5242880</option>
                     <option name="required">false</option>
                  </options>
               </field>
            </output>
            <tempFields>
               <field description="" name="lstMI" type="objectlist">
                  <options>
                     <option name="referenceTo">EIStaging:FPMT_MasterIndex</option>
                     <option hide="true" name="multiSelect">true</option>
                     <option name="required">false</option>
                     <option name="isCopy">true</option>
                     <option name="guid">4FnbhNBKiTXlsOjJuHLj8u</option>
                  </options>
               </field>
               <field description="" name="txtMI" type="string">
                  <options>
                     <option name="required">false</option>
                  </options>
               </field>
            </tempFields>
            <notes/>
            <deployment suspendOnFault="false" tracingLevel="verbose">
               <targetLocation>SecureAgent</targetLocation>
               <rest>
                  <allowAnonymousAccess/>
               </rest>
            </deployment>
            <flow id="a">
               <start id="b">
                  <link id="k86effm3" targetId="k86effm2"/>
               </start>
               <assignment id="k86effm2">
                  <title>Assignment to lstMI</title>
                  <operation source="list" to="temp.lstMI">
                     <query advanceQuery="false" from="EIStaging:FPMT_MasterIndex">
                        <where>ORDER BY Agency, MasterIndex LIMIT 9999999</where>
                     </query>
                  </operation>
                  <link id="k86effmc" targetId="k86effmb"/>
               </assignment>
               <assignment id="k86effmb">
                  <title>Assignment to Response</title>
                  <operation source="formula" to="temp.txtMI">
                     <expression language="XQuery">let $sources:=$temp.lstMI 
let $labels:=fn:distinct-values($sources/*/fn:node-name(.))
let $ArrayHeader:=('Biennium','Agency','MasterIndex','MasterIndexTitle')
return 
&lt;table&gt;
&lt;th&gt;
{
for $item in $ArrayHeader
return(:&lt;th&gt;{$item}&lt;/th&gt;:)
concat($item, ",")
},&amp;#xa;
&lt;/th&gt;

 
{
for$source in $sources
return&lt;tr&gt;{
for $item in $ArrayHeader
return concat(fn:data($source/*[fn:local-name-from-QName(fn:node-name())=$item]),  ',')
}&amp;#xa;&lt;/tr&gt;
}
&lt;/table&gt;</expression>
                  </operation>
                  <operation source="formula" to="output.Response">
                     <expression language="XQuery">sff:createAttachmentFromBase64('MI.csv', util:base64Encode($temp.txtMI, 'UTF-8'), 'application/csv')

</expression>
                  </operation>
                  <link id="k86effmn" targetId="k7548nqk"/>
               </assignment>
               <end id="k7548nqk">
                  <title>End</title>
               </end>
            </flow>
         </process>
      </types1:Entry>
