            <flow id="a">
               <start id="b">
                  <link id="k8f4q82s" targetId="k8f4q82r"/>
               </start>
               <assignment id="k8f4q82r">
                  <title>Assignment to lstVendor</title>
                  <operation source="list" to="temp.lstVendor">
                     <query advanceQuery="false" from="EIStaging:FPMT_StatewideVendorEdit">
                        <where>VendorAddress1 like '%,%' Order by VendorNumber, VendorName LIMIT 1000</where>
                     </query>
                  </operation>
                  <link id="k8f4q833" targetId="k8f4q832"/>
               </assignment>
               <assignment id="k8f4q832">
                  <title>Assignment to txtVendor, Response CSV</title>
                  <operation source="formula" to="temp.txtVendor">
                     <expression language="XQuery">let $sources:=$temp.lstVendor 
let $labels:=fn:distinct-values($sources/*/fn:node-name(.))
let $ArrayHeader:=('VendorNumber','VendorNumberSuffix','VendorName','VendorAddress1','VendorAddress2','VendorAddress3','VendorCity','VendorState')
return 
&lt;table&gt;
&lt;th&gt;
{
for $item in $ArrayHeader
return(:&lt;th&gt;{$item}&lt;/th&gt;:)
concat($item, ",")
},&amp;#xa;
&lt;/th&gt;

 
{
for$source in $sources
return&lt;tr&gt;{
for $item in $ArrayHeader
return concat(fn:data($source/*[fn:local-name-from-QName(fn:node-name())=$item]),  ',')
}&amp;#xa;&lt;/tr&gt;
}
&lt;/table&gt;</expression>
                  </operation>
                  <operation source="formula" to="output.Response">
                     <expression language="XQuery">sff:createAttachmentFromBase64('Vendor.csv', util:base64Encode($temp.txtVendor, 'UTF-8'), 'application/csv')</expression>
                  </operation>
                  <link id="k8f4q83f" targetId="c"/>
               </assignment>
               <end id="c"/>
            </flow>
         </process>
