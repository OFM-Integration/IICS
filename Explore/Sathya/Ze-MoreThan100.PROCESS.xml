   } 
&lt;/th&gt; 
{
for$source in $sources
return&lt;tr&gt;{
for$label in $labels
return concat(fn:data($source/*[fn:node-name()=$label]),',')
}&lt;/tr&gt;
}
&lt;/table&gt;</expression>
